            <kind>1</kind>
            <string>///Je parse et écris dans le fichier html (compressé)
//Variable temporaire, utilisé pour faire le tri correctement dans les images.
NumbOfImages=0;
//Placement initial
x= room_width-(object_button.sprite_width/2);
y=object_button.sprite_height/2;
//Go faire des collisions:

while(collision_point(x,y,object_part,false,true) != noone){
    This_part=collision_point(x,y,object_part,false,true);
    switch(This_part.image_index){
        
        case 1: //Écrire un titre:
            file_text_write_string(global.html,'&lt;p class="BigTitlePurple"&gt;'+This_part.Final+'&lt;/p&gt;');
            break;
        
        case 2: //Écrire un sous titre violet:
            file_text_write_string(global.html,'&lt;p class="SubTitlePurple"&gt;'+This_part.Final+'&lt;/p&gt;');
            break;
        
        case 3: //Écrire un sous titre :
            file_text_write_string(global.html,'&lt;p class="SubTitleDark"&gt;'+This_part.Final+'&lt;/p&gt;');
            break;
            
        case 4: //Écrire un petit titre violet:
            file_text_write_string(global.html,'&lt;p class="LittleTitlePurple"&gt;'+This_part.Final+'&lt;/p&gt;');
            break;
    
        case 5: //Écrire un petit titre:
            file_text_write_string(global.html,'&lt;p class="LittleTitle"&gt;'+This_part.Final+'&lt;/p&gt;');
            break;
        
        case 6: //Écrire un texte syntaxé:
            file_text_write_string(global.html,'&lt;p class="Syntax"&gt;'+This_part.Final+'&lt;/p&gt;');
            break;
            
        case 7: //Écrire un tableau:
            //Écrire la tête du tableau
            file_text_write_string(global.html,'&lt;table class="FunctionTable"&gt;&lt;tr class="FunctionTableTitle"&gt;&lt;td&gt;Arguments&lt;/td&gt;&lt;td&gt;Type&lt;/td&gt;&lt;td&gt;Description&lt;/td&gt;&lt;/tr&gt;');
            for(i=1;i&lt;=array_height_2d(This_part.Final);i++){
                if floor(i/2)*2 = i{
                    //Chiffre pair, penser à changer la couleur du fond de ces lignes:
                    file_text_write_string(global.html,'&lt;tr class="FunctionTableLineBis"&gt;&lt;td&gt;'+This_part.Final[i-1,0]+'&lt;/td&gt;&lt;td&gt;'+This_part.Final[i-1,1]+'&lt;/td&gt;&lt;td&gt;'+This_part.Final[i-1,0]+'&lt;/td&gt;&lt;/tr&gt;');
                    }
                else{
                    //Chiffre impaire ligne de couleur normale:
                    file_text_write_string(global.html,'&lt;tr&gt;&lt;td&gt;'+This_part.Final[i-1,0]+'&lt;/td&gt;&lt;td&gt;'+This_part.Final[i-1,1]+'&lt;/td&gt;&lt;td&gt;'+This_part.Final[i-1,0]+'&lt;/td&gt;&lt;/tr&gt;');
                    }
                }
            //Fini le tableau:
            file_text_write_string(global.html,"&lt;/table&gt;");
            break;
            
        case 8: //C'est une image, pas encore bien pris en charge à vrai dire. Faut que je les centre ou que je fasse des versions txt+img
            //Do something now;
            NumbOfImages++;
            //Vérification de la présence du fichier: Que des png
            if file_exists(working_directory+"images/"+string(NumbOfImages)+".png"){
                file_rename(working_directory+"images/"+string(NumbOfImages)+".png",working_directory+"images/"+global.SiteTitle+'-'+string(NumbOfImages)+".png");
                }
            //Maintenant faut écrire ET l'image ET le texte.
            if This_part.Final[0]=true{
                //Aligner l'image à gauche, le texte à droite:
                file_text_write_string(global.html,'&lt;img class="FloatLeft" src="../images/'+global.SiteTitle+'-'+string(NumbOfImages)+'.png"/&gt;');
                file_text_write_string(global.html,'&lt;p class="Right"&gt;'+This_part.Final[1]+'&lt;/p&gt;');
                }
            else{   
                //Aligner l'image à droite, le texte à gauche:
                file_text_write_string(global.html,'&lt;img class="FloatRight" src="../images/'+global.SiteTitle+'-'+string(NumbOfImages)+'.png"/&gt;');
                file_text_write_string(global.html,'&lt;p class="Left"&gt;'+This_part.Final[1]+'&lt;/p&gt;');
                }
            break;
        
        case 9: //C'est du texte, ou un paragraphe plutôt:
            file_text_write_string(global.html,This_part.Final);
            break;
        
        }
    y+=object_button.sprite_height;
    }

//NORMALEMENT Il n'y a plus de parties à compiler:fermons le fichier.
//Bas de page:
file_text_write_string(global.html,'&lt;p class="foot"&gt;&lt;a href="../index.html"&gt;Back to the index&lt;/a&gt;&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;');
file_text_close(global.html);
//Maintenant il faudrait générer un index des pages existantes. On doit supprimer l'index précédent s'il existe.
if file_exists(working_directory+"index.html"){
    file_rename(working_directory+"index.html",working_directory+"index.txt");
    file_delete(working_directory+"index.txt");
    }
index=file_text_open_write(working_directory+"index.html");
//On écrit le haut de page: compressé.
file_text_write_string(index,'&lt;DOCTYPE html&gt;&lt;html&gt;&lt;head&gt;&lt;meta charset="utf-8" /&gt;&lt;link rel="stylesheet" href="css/css.css" /&gt;&lt;title&gt;index-PPA Docs&lt;/title&gt;&lt;/head&gt;&lt;body&gt;&lt;p class="BigTitlePurple"&gt;Index-PPA Docs&lt;/p&gt;&lt;p class="SubTitlePurple"&gt;This section shows every page available about this game.&lt;/p&gt;');
if file_find_first(working_directory+"html/*.html",0) != ""{
    file_text_write_string(index,'&lt;ul&gt;&lt;li&gt;&lt;a href="html/'+file_find_first(working_directory+"html/*.html",0)+'"&gt;'+string_replace(file_find_first(working_directory+"html/*.html",0),".html","")+'&lt;/a&gt;&lt;/li&gt;');
    for(i=1;i&gt;-1;i++){
        temp=file_find_next();
        if temp != ""{
            file_text_write_string(index,'&lt;li&gt;&lt;a href="html/'+temp+'"&gt;'+string_replace(temp,".html","")+'&lt;/a&gt;&lt;/li&gt;');
            }
        else{
            i = -40;
            }
        }
    //Fin de l'index:
    file_text_write_string(index,'&lt;/ul&gt;&lt;/body&gt;&lt;/html&gt;');
    }

file_text_close(index);
instance_destroy();
    
</string>
          </argument>
        </arguments>
      </action>
    </event>
