        if (!place_meeting(x,y+sign(vsp),object_wall_platforms))
            {
            y += sign(vsp);
            }
        else
            {
            dir *= -1;
            }
        }
    
        
    
    //Making it one way platform
    if (instance_exists(object_player))
        {
        if round(object_player.y + (object_player.sprite_height/2)) &gt; y || (gamepad_axis_value(object_player.ID,gp_axislv)) &gt; 0.70
            {
            mask_index = -1;
            object_player.vsp_carry = 0;
            }
        else
            {
            mask_index = my_sprite;
            image_index = my_img_index;
            if place_meeting(x,yprevious-1,object_player){
                object_player.vsp_carry = y-yprevious;
                object_player.vsp = 0;
                object_player.y = yprevious-ceil(object_player.sprite_height/2)-1;
                }
            }
        }
        
    }
</string>
