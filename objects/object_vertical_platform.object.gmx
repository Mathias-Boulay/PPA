            <string>if !instance_exists(object_pause){
    //Re-asseting good collision mask for... collisions.
    mask_index = my_sprite;
    image_index = my_img_index;
    
    //moving
    vsp = dir*movespeed;
            
    if (!place_meeting(x,y+vsp,object_wall_platforms))
